{
  "name": "movie-lib",
  "version": "1.0.0",
  "description": "Movie Library API for a technical challenge",
  "main": "app.js",
  "scripts": {
    "build": "tsc --build --verbose",
    "start": "cross-env NODE_ENV=production node ./dist/app.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/app.ts",
    "test": "cross-env NODE_ENV=test jest --collect-coverage",
    "test-staged": "cross-env NODE_ENV=test jest --collect-coverage -o",
    "format": "prettier --write --ignore-unknown src/**/*",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "migrator": "node ./cli/umzug/migrate",
    "seeder": "node ./cli/umzug/seed"
  },
  "lint-staged": {
    "src/**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lunarisnia/movie-lib.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lunarisnia/movie-lib/issues"
  },
  "homepage": "https://github.com/Lunarisnia/movie-lib#readme",
  "dependencies": {
    "@graphql-tools/executor-http": "^0.1.4",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.15",
    "class-validator": "^0.14.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^3.5.1",
    "joi": "^17.7.0",
    "morgan": "~1.9.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/validator": "^13.7.11",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.3",
    "sequelize-cli": "^6.5.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
